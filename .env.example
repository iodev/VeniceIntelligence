# Venice Intelligence - Environment Variables Template
# Copy this file to .env and fill in your actual API keys

# Venice.ai API Configuration
VENICE_API_KEY=your-venice-api-key-here

# OpenAI Configuration (for embeddings)
OPENAI_API_KEY=your-openai-api-key-here

# Perplexity API Configuration
PERPLEXITY_API_KEY=your-perplexity-api-key-here

# Anthropic API Configuration
ANTHROPIC_API_KEY=your-anthropic-api-key-here

# Hugging Face API Configuration
HUGGING_FACE_API_KEY_IO=your-huggingface-api-key-here

# Qdrant Vector Database Configuration
QDRANT_URL=your-qdrant-url-here
QDRANT_API_KEY=your-qdrant-api-key-here
QDRANT_COLLECTION_NAME=agent_memory

# Flask Configuration
SESSION_SECRET=your-session-secret-key-here
FLASK_ENV=development

# Database Configuration (if using external database)
DATABASE_URL=sqlite:///venice_intelligence.db